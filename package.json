  "scripts": {
    "build": "env $(cat $(if [ \"$IS_STAGING\" = \"true\" ]; then echo .env.staging; elif [ \"$IS_ETH\" = \"true\" ]; then echo .env.eth; else echo .env; fi) | grep -v \"#\" | xargs) npx @nestjs/cli build",
    "start": "env $(cat $(if [ \"$IS_STAGING\" = \"true\" ]; then echo .env.staging; elif [ \"$IS_ETH\" = \"true\" ]; then echo .env.eth; else echo .env; fi) | grep -v \"#\" | xargs) npx @nestjs/cli start -e 'node --experimental-require-module'",
    "start:development": "env $(cat $(if [ \"$IS_STAGING\" = \"true\" ]; then echo .env.staging; elif [ \"$IS_ETH\" = \"true\" ]; then echo .env.eth; else echo .env; fi) | grep -v \"#\" | xargs) npx @nestjs/cli start --watch -e 'node --experimental-require-module'",
    "start:debug": "env $(cat $(if [ \"$IS_STAGING\" = \"true\" ]; then echo .env.staging; elif [ \"$IS_ETH\" = \"true\" ]; then echo .env.eth; else echo .env; fi) | grep -v \"#\" | xargs) npx @nestjs/cli start --debug --watch",
    "start:production": "env $(cat $(if [ \"$IS_STAGING\" = \"true\" ]; then echo .env.staging; elif [ \"$IS_ETH\" = \"true\" ]; then echo .env.eth; else echo .env; fi) | grep -v \"#\" | xargs) NODE_ENV=production node --experimental-require-module build/main",
    "dev": "npm run start:development",
    "start:dev": "npm run start:development",
    "start:prod": "npm run start:production",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
